{
    "compilerOptions": {
        "target": "ES2019",
        "lib": [
            "ES2019"
        ],
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "declaration": true,
        "declarationMap": true,
        "noEmit": true
    },
    "include": [
        "src"
    ]
}